<svg width="640" height="1136" viewBox="0 0 640 1136" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="640" height="1136" fill="#f9fafb"/>
  
  <!-- Header -->
  <rect x="0" y="0" width="640" height="80" fill="#1f2937"/>
  <text x="20" y="50" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">FieldWork</text>
  
  <!-- Map Area -->
  <rect x="20" y="100" width="600" height="400" fill="#e5e7eb" stroke="#9ca3af" stroke-width="2" stroke-dasharray="5,5"/>
  <text x="320" y="300" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="16">Mobile Map View</text>
  
  <!-- Plot markers -->
  <circle cx="150" cy="200" r="6" fill="#ef4444"/>
  <circle cx="300" cy="250" r="6" fill="#ef4444"/>
  <circle cx="450" cy="180" r="6" fill="#ef4444"/>
  
  <!-- Plot Details Card -->
  <rect x="20" y="520" width="600" height="200" fill="#ffffff" stroke="#d1d5db" stroke-width="1" rx="8"/>
  <text x="40" y="550" fill="#374151" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Plot A-1-15</text>
  <text x="40" y="580" fill="#6b7280" font-family="Arial, sans-serif" font-size="14">Status: Active</text>
  <text x="40" y="600" fill="#6b7280" font-family="Arial, sans-serif" font-size="14">Created: 2024-01-15</text>
  
  <!-- Action Buttons -->
  <rect x="40" y="650" width="120" height="40" fill="#3b82f6" rx="4"/>
  <text x="100" y="675" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14">Edit</text>
  
  <rect x="180" y="650" width="120" height="40" fill="#ef4444" rx="4"/>
  <text x="240" y="675" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14">Delete</text>
  
  <!-- Bottom Navigation -->
  <rect x="0" y="1050" width="640" height="86" fill="#ffffff" stroke="#d1d5db" stroke-width="1"/>
  <text x="160" y="1095" text-anchor="middle" fill="#374151" font-family="Arial, sans-serif" font-size="12">Map</text>
  <text x="320" y="1095" text-anchor="middle" fill="#374151" font-family="Arial, sans-serif" font-size="12">Plots</text>
  <text x="480" y="1095" text-anchor="middle" fill="#374151" font-family="Arial, sans-serif" font-size="12">Settings</text>
</svg>
